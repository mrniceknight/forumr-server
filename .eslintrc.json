{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["google", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
